services:

  web_debugger:
    image: vscoder/webdebugger:{{ deploy_web_debugger_image_version }}
    hostname: web_debugger
    container_name: web_debugger
    restart: always
    mem_limit: 100M
    ports:
      - 8080:8080
    environment:
      - APP_BGCOLOR={{ deploy_web_debugger_background_color }}
 